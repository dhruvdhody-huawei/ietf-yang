       |        |       rt-types:ip-multicast-group-address
       |        +--rw authentication
       |        |  +--rw (authentication-type)?
       |        |     +--:(symmetric-key)
       |        |        +--rw key-id?   leafref
       |        +--rw ttl?              uint8
       |        +--rw minclock?         uint8
       |        +--rw maxclock?         uint8
       |        +--rw beacon?           uint8
